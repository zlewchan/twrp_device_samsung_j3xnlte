# mount point       fstype    device                                                                flags
/sdcard1         vfat     /dev/block/mmcblk1p1                               flags=display="Sdcard1"
/system          ext4     /dev/block/platform/sdio_emmc/by-name/SYSTEM       flags=display="System";backup=1
/preload         ext4     /dev/block/platform/sdio_emmc/by-name/HIDDEN       flags=display="Preload"
/data            ext4     /dev/block/platform/sdio_emmc/by-name/userdata     flags=display="Data"
/cache           ext4     /dev/block/platform/sdio_emmc/by-name/CACHE        flags=display="Cache"
/efs             ext4     /dev/block/platform/sdio_emmc/by-name/efs          flags=display="Efs"
/productinfo     ext4     /dev/block/platform/sdio_emmc/by-name/prodnv       flags=display="Productinfo"
/boot            emmc     /dev/block/platform/sdio_emmc/by-name/KERNEL       flags=display="Boot";backup=1;flashimg=1
/recovery        emmc     /dev/block/platform/sdio_emmc/by-name/RECOVERY     flags=display="Recovery";backup=1;flashimg=1
/l_modem         emmc     /dev/block/platform/sdio_emmc/by-name/l_modem      flags=display="L_modem"
/l_ldsp          emmc     /dev/block/platform/sdio_emmc/by-name/l_ldsp       flags=display="L_ldsp"
/l_gdsp          emmc     /dev/block/platform/sdio_emmc/by-name/l_gdsp       flags=display="L_gdsp"
/persdata        ext4     /dev/block/platform/sdio_emmc/by-name/PERSDATA     flags=display="Persdata"
/pmsys           emmc     /dev/block/platform/sdio_emmc/by-name/pm_sys       flags=display="Pmsys"
/l_warm          emmc     /dev/block/platform/sdio_emmc/by-name/l_warm       flags=display="L_warm"
/nvitem          emmc     /dev/block/platform/sdio_emmc/by-name/l_fixnv2     flags=display="Nvitem"
